{
  "name": "dev-directory",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && ttsc && next build",
    "dev:server": "cross-env NODE_ENV=development npm run ts:exec server/index.ts",
    "dev": "nodemon",
    "docker:db": "docker-compose exec pg psql -U postgres -d dev-directory",
    "docker:deps": "docker-compose up",
    "heroku-postbuild": "npm run build && npx sequelize-cli db:migrate",
    "jest": "cross-env NODE_ENV=test npx jest --runInBand",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "migrate": "npm run sequelize-cli db:migrate",
    "migrate:undo": "npm run sequelize-cli db:migrate:undo",
    "sequelize-cli": "cross-env NODE_ENV=development sequelize-cli",
    "start": "node dist/server/index",
    "test:server": "npm run jest -- ./server",
    "test:watch": "npm run jest -- --watch",
    "test": "npm run jest",
    "ts:exec": "ts-node -r tsconfig-paths/register --project ./tsconfig.json --transpile-only",
    "ts:check": "npx tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timmyichen/dev-directory.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timmyichen/dev-directory/issues"
  },
  "homepage": "https://github.com/timmyichen/dev-directory#readme",
