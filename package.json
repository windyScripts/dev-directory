    "@mui/material": "^5.11.4",
    "@types/express": "^4.17.15",
    "axios": "^1.2.3",
    "concurrently": "^7.6.0",
    "cookie-parser": "^1.4.6",
    "discord-oauth2": "^2.11.0",
    "dotenv": "^16.0.3",
    "dotenv-flow": "^3.2.0",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-async-router": "^0.1.15",
    "express-response-errors": "^1.0.5",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "next": "^12.1.2",
    "next-absolute-url": "^1.2.2",
